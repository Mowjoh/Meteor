<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Meteor"
        xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="Meteor.MainWindow"
        mc:Ignorable="d"
        Title="Meteor" Height="750" Width="1024" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Foreground="#FF4B4A4A">
    <Window.Resources>
        <Style x:Key="FocusVisual">
            <Setter Property="Control.Template">
                <Setter.Value>
                    <ControlTemplate>
                        <Rectangle Margin="2" SnapsToDevicePixels="true" Stroke="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}" StrokeThickness="1" StrokeDashArray="1 2"/>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <SolidColorBrush x:Key="Button.Static.Background" Color="#FFDDDDDD"/>
        <SolidColorBrush x:Key="Button.Static.Border" Color="#FF707070"/>
        <SolidColorBrush x:Key="Button.MouseOver.Background" Color="#FFBEE6FD"/>
        <SolidColorBrush x:Key="Button.MouseOver.Border" Color="#FF3C7FB1"/>
        <SolidColorBrush x:Key="Button.Pressed.Background" Color="#FFC4E5F6"/>
        <SolidColorBrush x:Key="Button.Pressed.Border" Color="#FF2C628B"/>
        <SolidColorBrush x:Key="Button.Disabled.Background" Color="#FFF4F4F4"/>
        <SolidColorBrush x:Key="Button.Disabled.Border" Color="#FFADB2B5"/>
        <SolidColorBrush x:Key="Button.Disabled.Foreground" Color="#FF838383"/>
        <Style x:Key="TopButtons" TargetType="{x:Type Button}">
            <Setter Property="FocusVisualStyle" Value="{StaticResource FocusVisual}"/>
            <Setter Property="Background" Value="{StaticResource Button.Static.Background}"/>
            <Setter Property="BorderBrush" Value="{StaticResource Button.Static.Border}"/>
            <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"/>
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="Padding" Value="1"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border x:Name="border" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" SnapsToDevicePixels="true">
                            <ContentPresenter x:Name="contentPresenter" Focusable="False" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsDefaulted" Value="true">
                                <Setter Property="BorderBrush" TargetName="border" Value="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}"/>
                            </Trigger>
                            <Trigger Property="IsMouseOver" Value="true">
                                <Setter Property="Background" TargetName="border" Value="{StaticResource Button.MouseOver.Background}"/>
                                <Setter Property="BorderBrush" TargetName="border" Value="{StaticResource Button.MouseOver.Border}"/>
                            </Trigger>
                            <Trigger Property="IsPressed" Value="true">
                                <Setter Property="Background" TargetName="border" Value="{StaticResource Button.Pressed.Background}"/>
                                <Setter Property="BorderBrush" TargetName="border" Value="{StaticResource Button.Pressed.Border}"/>
                            </Trigger>
                            <Trigger Property="IsEnabled" Value="false">
                                <Setter Property="Background" TargetName="border" Value="{StaticResource Button.Disabled.Background}"/>
                                <Setter Property="BorderBrush" TargetName="border" Value="{StaticResource Button.Disabled.Border}"/>
                                <Setter Property="TextElement.Foreground" TargetName="contentPresenter" Value="{StaticResource Button.Disabled.Foreground}"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="columnstyle" TargetType="{x:Type DataGridColumnHeader}">
            <Setter Property="Background" Value="Transparent"/>
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="FontSize" Value="18" />
            <Setter Property="Padding" Value="15,0"/>

        </Style>
        <Style x:Key="scrollstyle" TargetType="{x:Type ScrollViewer}">
            <Setter Property="Background" Value="Transparent"/>
            <Setter Property="Foreground" Value="White"/>
        </Style>
    </Window.Resources>
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="#FF2E2E2E" Offset="0.12"/>
            <GradientStop Color="#FF0B686C" Offset="0.927"/>
        </LinearGradientBrush>
    </Window.Background>

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="75*"/>
            <ColumnDefinition Width="14*"/>
            <ColumnDefinition Width="420*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="540*"/>
            <RowDefinition Height="115*"/>
            <RowDefinition Height="66*"/>
        </Grid.RowDefinitions>
        <ListBox x:Name="Section_List" HorizontalAlignment="Left" Height="506" VerticalAlignment="Top" Width="157" Background="{x:Null}" BorderBrush="{x:Null}" Grid.ColumnSpan="2" SelectionChanged="section_change">
            <ListBoxItem Content="Skins" FontFamily="Segoe UI Semibold" FontSize="20" Foreground="White" Margin="0,30,0,10"/>
            <Separator/>
            <ListBoxItem Content="Stages" FontFamily="Segoe UI Semibold" FontSize="20" Foreground="White" Margin="0,10,0,20"/>
            <Separator/>
            <ListBoxItem Content="Interface" FontFamily="Segoe UI Semibold" FontSize="20" Foreground="White" Margin="0,10,0,20"/>
            <Separator/>
            <ListBoxItem Content="FileBank" FontFamily="Segoe UI Semibold" FontSize="20" Foreground="White" Margin="0,10,0,20"/>
            <Separator/>
            <ListBoxItem Content="Workspace" FontFamily="Segoe UI Semibold" FontSize="20" Foreground="White" Margin="0,10,0,20"/>
            <Separator/>
            <ListBoxItem Content="Configuration" FontFamily="Segoe UI Semibold" FontSize="20" Foreground="White" Margin="0,10,0,20"/>
            <Separator Margin="0"/>
            <ListBoxItem Content="About" FontFamily="Segoe UI Semibold" FontSize="20" Foreground="White" Margin="0,10,0,0"/>
        </ListBox>
        <TextBlock x:Name="Console" HorizontalAlignment="Left" Grid.Row="1" TextWrapping="Wrap" VerticalAlignment="Top" Height="115" Width="1018" Grid.ColumnSpan="3" Background="#7F000000" Foreground="White"/>
        <Separator HorizontalAlignment="Left" Height="27" Margin="-78,251,0,0" VerticalAlignment="Top" Width="484" RenderTransformOrigin="0.5,0.5" Grid.ColumnSpan="3">
            <Separator.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-90"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Separator.RenderTransform>
        </Separator>
        <StatusBar x:Name="Status_Bar" Grid.ColumnSpan="3" HorizontalAlignment="Left" Height="66" Grid.Row="2" VerticalAlignment="Top" Width="1018" Background="#4C000000">
            <Label x:Name="label" Content="Status :" Background="{x:Null}" Foreground="White" FontSize="15"/>
            <Label x:Name="label_Copy" Content="app_status" Background="{x:Null}" Foreground="White" FontSize="15"/>
            <ProgressBar Height="10" Width="300" MinHeight="20"/>
            <Label x:Name="label_Copy1" Content="Operation :" Background="{x:Null}" Foreground="White" FontSize="15" Margin="30,0,0,0"/>
            <Label x:Name="label_Copy2" Content="operation_status" Background="{x:Null}" Foreground="White" FontSize="15"/>
            <ProgressBar Height="10" Width="300" MinHeight="20"/>
        </StatusBar>
        <TabControl x:Name="TabControl_Sections" Grid.Column="2" HorizontalAlignment="Left" Height="540" VerticalAlignment="Top" Width="840" Foreground="White" BorderBrush="{x:Null}" Background="{x:Null}">
            <TabItem x:Name="Skins" Header="TabItem" Background="{x:Null}" BorderBrush="{x:Null}" Margin="-2,-2,-2,0" Visibility="Collapsed">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="9*"/>
                        <RowDefinition Height="247*"/>
                        <RowDefinition Height="256*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="150*"/>
                        <ColumnDefinition Width="30*"/>
                        <ColumnDefinition Width="192*"/>
                        <ColumnDefinition Width="29*"/>
                        <ColumnDefinition Width="202*"/>
                        <ColumnDefinition Width="231*"/>
                    </Grid.ColumnDefinitions>
                    <ListBox x:Name="CharacterListBox" Foreground="White" Background="{x:Null}" BorderBrush="{x:Null}" Grid.RowSpan="2" Margin="0,37,0,30" Grid.Row="1" ScrollViewer.VerticalScrollBarVisibility="Hidden" ScrollViewer.HorizontalScrollBarVisibility="Hidden" SelectionChanged="character_selected" ToolTip="Select a character you want to edit"/>
                    <ListBox x:Name="Skinslistbox" Grid.Column="2" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" Grid.RowSpan="2" Margin="0,37,0,56" Grid.Row="1" SelectionChanged="skin_selected" ToolTip="Select a skin you want to edit"/>
                    <Separator HorizontalAlignment="Left" Margin="-76,231,0,245" Width="484" RenderTransformOrigin="0.5,0.5" Grid.ColumnSpan="5" Grid.RowSpan="2" Grid.Row="1">
                        <Separator.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform/>
                                <SkewTransform/>
                                <RotateTransform Angle="-90"/>
                                <TranslateTransform/>
                            </TransformGroup>
                        </Separator.RenderTransform>
                    </Separator>
                    <Separator HorizontalAlignment="Left" Height="27" Margin="144,230,0,0" VerticalAlignment="Top" Width="484" RenderTransformOrigin="0.5,0.5" Grid.ColumnSpan="6" Grid.RowSpan="2" Grid.Row="1">
                        <Separator.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform/>
                                <SkewTransform/>
                                <RotateTransform Angle="-90"/>
                                <TranslateTransform/>
                            </TransformGroup>
                        </Separator.RenderTransform>
                    </Separator>
                    <GroupBox x:Name="skins_skin_info_box" Grid.Column="4" Header="Skin Information" Foreground="White" Grid.ColumnSpan="2" Margin="0,0,10,0" Grid.RowSpan="2">
                        <Grid Margin="0,0,-2,0">
                            <Label x:Name="skins_skin_name_label" Content="Skin Name" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White"/>
                            <TextBox x:Name="skins_skin_name" HorizontalAlignment="Left" Height="23" Margin="169,14,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="242" KeyDown="skin_name_save" ToolTip="The Skin's name. Press Enter to save the changes"/>
                            <Label x:Name="skins_skin_slot_label" Content="Slot" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White"/>
                            <TextBox x:Name="skins_slot" HorizontalAlignment="Left" Height="23" Margin="169,45,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="242" IsReadOnly="True" ToolTip="The slot the skin will be put in"/>
                            <Label x:Name="skins_skin_author_label" Content="Author" HorizontalAlignment="Left" Margin="10,73,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White"/>
                            <TextBox x:Name="skins_author" HorizontalAlignment="Left" Height="23" Margin="169,76,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="242" KeyDown="skin_author_save" ToolTip="The Skin's Author. Press Enter to validate the change"/>
                            <Label x:Name="skins_skin_nameplate_label" Content="Nameplate" HorizontalAlignment="Left" Margin="10,104,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White"/>
                            <ComboBox x:Name="skins_skin_nameplate" HorizontalAlignment="Left" VerticalAlignment="Top" Width="242" Margin="169,108,0,0" IsEnabled="False" ToolTip="Select the nameplate you want for that slot">
                                <System:String>Voice</System:String>
                            </ComboBox>
                            <Label x:Name="skins_skin_voicebank_label" Content="Voicebank" HorizontalAlignment="Left" Margin="10,135,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White"/>
                            <ComboBox x:Name="skins_skin_voicebank" HorizontalAlignment="Left" Margin="169,139,0,0" VerticalAlignment="Top" Width="242" IsEnabled="False" ToolTip="Select the voicebank you want for that slot"/>
                            <Button x:Name="mod_button" Content="Mod page" HorizontalAlignment="Left" Margin="278,204,0,0" VerticalAlignment="Top" Width="125" Click="mod_link" Visibility="Hidden"/>
                            <Button x:Name="author_button" Content="Author page" HorizontalAlignment="Left" Margin="10,204,0,0" VerticalAlignment="Top" Width="125" Click="mod_author" Visibility="Hidden"/>
                            <Button x:Name="convert_button" Content="Convert to custom skin" HorizontalAlignment="Left" Margin="10,166,0,0" VerticalAlignment="Top" Width="139"/>
                        </Grid>
                    </GroupBox>
                    <GroupBox x:Name="skins_skin_files_box" Grid.Column="4" Header="Skin Files" HorizontalAlignment="Left" Height="256" Grid.Row="2" VerticalAlignment="Top" Width="423" Foreground="White" Grid.ColumnSpan="2">
                        <Grid Margin="0,0,-2,0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="210*"/>
                                <ColumnDefinition Width="211*"/>
                            </Grid.ColumnDefinitions>
                            <Label x:Name="skin_skin_files_model_label" Content="Model" HorizontalAlignment="Left" VerticalAlignment="Top" Background="{x:Null}" Foreground="White"/>
                            <Label x:Name="skin_skin_files_chr_label" Content="Chr" HorizontalAlignment="Left" VerticalAlignment="Top" Background="{x:Null}" Foreground="White" Grid.Column="1" Margin="157,0,0,0"/>
                            <Separator HorizontalAlignment="Left" Height="27" Margin="93,101,0,0" VerticalAlignment="Top" Width="236" RenderTransformOrigin="0.5,0.5" Grid.ColumnSpan="2">
                                <Separator.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform/>
                                        <SkewTransform/>
                                        <RotateTransform Angle="-90"/>
                                        <TranslateTransform/>
                                    </TransformGroup>
                                </Separator.RenderTransform>
                            </Separator>
                        </Grid>
                    </GroupBox>
                    <Button x:Name="add_skin_button" Content="Add Skin Slot" Grid.Column="2" HorizontalAlignment="Left" Margin="10,226,0,0" Grid.Row="2" VerticalAlignment="Top" Width="172" Click="add_skin" ToolTip="Press this button to add a new skin slot !"/>
                    <Label x:Name="character_header" Content="Characters" HorizontalAlignment="Left" Margin="10,1,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White" FontStyle="Italic" FontSize="16" Grid.Row="1"/>
                    <Label x:Name="skin_header" Content="Skins" HorizontalAlignment="Left" Margin="10,1,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White" Grid.Column="2" FontStyle="Italic" FontSize="16" Grid.Row="1"/>
                </Grid>
            </TabItem>
            <TabItem x:Name="Stages" Header="TabItem" Background="{x:Null}" BorderBrush="{x:Null}" Margin="-2,-2,-2,0" Visibility="Collapsed">
                <Grid>
                    <Label x:Name="label4" Content="This section is under construction, come back later!" HorizontalAlignment="Left" Margin="47,233,0,0" VerticalAlignment="Top" Foreground="White" FontSize="22" FontFamily="Super Mario 256" ToolTip="Yeah, it actually means that. Basically I've not yet finished this section so I'm hiding it with this text. Yeah, pretty much all there is to that."/>
                </Grid>
            </TabItem>
            <TabItem x:Name="Interface" Header="TabItem" Background="{x:Null}" BorderBrush="{x:Null}" Margin="-2,-2,-2,0" Visibility="Collapsed">
                <Grid>
                    <Label x:Name="label6" Content="This section is under construction, come back later!" HorizontalAlignment="Left" Margin="47,233,0,0" VerticalAlignment="Top" Foreground="White" FontSize="22" FontFamily="Super Mario 256" ToolTip="This is the same thing as the stages section. Really. Changing sections again won't make it appear. or would it?"/>
                </Grid>
            </TabItem>
            <TabItem x:Name="FileBank" Header="TabItem" Background="{x:Null}" BorderBrush="{x:Null}" Margin="-2,-2,-2,0" Visibility="Collapsed">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="127*"/>
                        <ColumnDefinition Width="707*"/>
                    </Grid.ColumnDefinitions>
                    <Label x:Name="filebank_action_label" Content="Category" HorizontalAlignment="Left" VerticalAlignment="Top" Background="{x:Null}" Foreground="White" FontStyle="Italic" FontSize="16"/>
                    <ListBox x:Name="filebank_action_list" Foreground="White" Background="{x:Null}" BorderBrush="{x:Null}" Margin="0,46,42,0" ToolTip="Select a content category">
                        <ListBoxItem Content="Skins"/>
                    </ListBox>
                    <Separator x:Name="Content_Separator" HorizontalAlignment="Left" Height="27" Margin="-136,244,0,0" VerticalAlignment="Top" Width="484" RenderTransformOrigin="0.5,0.5" Grid.ColumnSpan="2">
                        <Separator.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform/>
                                <SkewTransform/>
                                <RotateTransform Angle="-90"/>
                                <TranslateTransform/>
                            </TransformGroup>
                        </Separator.RenderTransform>
                    </Separator>
                    <TabControl x:Name="filebank_content_area" HorizontalAlignment="Left" Height="498" VerticalAlignment="Top" Width="697" Background="{x:Null}" BorderBrush="#7F000000" Foreground="White" Margin="118,14,0,0" Grid.ColumnSpan="2" ToolTip="you can manage the content here">
                        <TabItem x:Name="filebank_skin_tab" Header="TabItem" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" Visibility="Collapsed">
                            <Grid Margin="0,3,0,-3">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="59*"/>
                                    <RowDefinition Height="359*"/>
                                </Grid.RowDefinitions>
                                <Label x:Name="filebank_character_label" Content="Choose a Character" HorizontalAlignment="Left" VerticalAlignment="Top" Background="{x:Null}" Foreground="White" FontStyle="Italic" FontSize="16" Margin="10,10,0,0"/>
                                <ComboBox x:Name="filebank_character_select" HorizontalAlignment="Left" Margin="184,17,0,0" VerticalAlignment="Top" Width="162" ToolTip="Filter the mods by character"/>
                                <DataGrid x:Name="filebank_skins_datagrid" Grid.Row="1"/>
                            </Grid>
                        </TabItem>
                        <TabItem x:Name="filebank_stage_tab" Header="TabItem" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" Margin="-2,-2,-2,0" Visibility="Collapsed">
                            <Grid/>
                        </TabItem>
                        <TabItem x:Name="filebank_nameplate_tab" Header="TabItem" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" Margin="-2,-2,-2,0" Visibility="Collapsed">
                            <Grid/>
                        </TabItem>
                        <TabItem x:Name="filebank_voicebank_tab" Header="TabItem" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" Margin="-2,-2,-2,0" Visibility="Collapsed">
                            <Grid/>
                        </TabItem>
                        <TabItem x:Name="filebank_css_icons_tab" Header="TabItem" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" Margin="-2,-2,-2,0" Visibility="Collapsed">
                            <Grid/>
                        </TabItem>
                    </TabControl>
                </Grid>
            </TabItem>
            <TabItem x:Name="Workspace" Header="TabItem" Background="{x:Null}" BorderBrush="{x:Null}" Margin="0,0,-4,-2" Visibility="Collapsed">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="255*"/>
                        <RowDefinition Height="257*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="196*"/>
                        <ColumnDefinition Width="311*"/>
                        <ColumnDefinition Width="327*"/>
                    </Grid.ColumnDefinitions>
                    <ListBox x:Name="workspace_listbox" Foreground="White" Background="{x:Null}" BorderBrush="{x:Null}" Margin="0,46,46,47" ScrollViewer.VerticalScrollBarVisibility="Hidden" ScrollViewer.HorizontalScrollBarVisibility="Hidden" ToolTip="Select a character you want to edit" Grid.RowSpan="2" SelectionChanged="workspace_selected"/>
                    <Label x:Name="workspace_list_header" Content="Workspaces" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White" FontStyle="Italic" FontSize="16" Height="31" Width="92"/>
                    <Separator HorizontalAlignment="Left" Margin="-76,240,0,245" Width="484" RenderTransformOrigin="0.5,0.5" Grid.ColumnSpan="2" Grid.RowSpan="2">
                        <Separator.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform/>
                                <SkewTransform/>
                                <RotateTransform Angle="-90"/>
                                <TranslateTransform/>
                            </TransformGroup>
                        </Separator.RenderTransform>
                    </Separator>
                    <GroupBox x:Name="workspace_information" Grid.Column="1" Header="Information" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="White" Height="235" Width="291">
                        <Grid>
                            <Label x:Name="label1_Copy" Content="Workspace name :" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White"/>
                            <TextBox x:Name="workspace_name_textbox" HorizontalAlignment="Left" Height="23" Margin="10,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="259" Text="Default Workspace"/>
                            <Button x:Name="workspace_save_name" Content="Save Name" HorizontalAlignment="Left" Margin="10,78,0,0" VerticalAlignment="Top" Width="259" Click="set_workspace_name"/>
                            <Button x:Name="workspace_set_active" Content="Set Active Workspace" HorizontalAlignment="Left" Margin="10,113,0,0" VerticalAlignment="Top" Width="259" Click="set_active_workspace" Visibility="Hidden"/>
                        </Grid>
                    </GroupBox>
                    <GroupBox x:Name="workspace_content" Grid.Column="2" Header="Content" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="White" Height="492" Width="291" Grid.RowSpan="2">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="47*"/>
                                <ColumnDefinition Width="46*"/>
                            </Grid.ColumnDefinitions>
                            <Label x:Name="label1" Content="Skin Count" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White"/>
                            <Label x:Name="label1_Copy1" Content="Nameplate Count" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White"/>
                            <Label x:Name="label1_Copy2" Content="Voicebank Count" HorizontalAlignment="Left" Margin="10,72,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White"/>
                            <Label x:Name="label1_Copy6" Content="Stage Count" HorizontalAlignment="Left" Margin="10,103,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White"/>
                            <Label x:Name="label1_Copy7" Content="Modded Interface" HorizontalAlignment="Left" Margin="10,134,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White"/>
                            <Label x:Name="label1_Copy8" Content="Modded CSS" HorizontalAlignment="Left" Margin="10,165,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White"/>
                            <Label x:Name="label1_Copy9" Content="Modded SSS" HorizontalAlignment="Left" Margin="10,196,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White"/>
                            <Label x:Name="label1_Copy10" Content="Most Skins" HorizontalAlignment="Left" Margin="11,260,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White"/>
                            <Label x:Name="label1_Copy11" Content="Most Stages" HorizontalAlignment="Left" Margin="11,291,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White"/>
                            <Label x:Name="label1_Copy12" Content="Build Count" HorizontalAlignment="Left" Margin="11,351,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White"/>
                            <Label x:Name="label1_Copy13" Content="Date created" HorizontalAlignment="Left" Margin="11,382,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="White"/>
                            <Label x:Name="skin_count_text" Content="0" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="White" Grid.Column="1" RenderTransformOrigin="-1.812,0.654"/>
                        </Grid>
                    </GroupBox>
                    <GroupBox x:Name="workspace_actions" Grid.Column="1" Header="Actions" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="White" Height="235" Width="291" Grid.Row="1">
                        <Grid>
                            <Button x:Name="workspace_clear" Content="Clear Workspace" HorizontalAlignment="Left" Margin="10,183,0,0" VerticalAlignment="Top" Width="259" Click="workspace_clear_Click"/>
                            <Button x:Name="button1_Copy" Content="Import Workspace from Sm4shExplorer" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="259" IsEnabled="False"/>
                            <Button x:Name="button1_Copy1" Content="Import Workspace from Meteor Skin Library" HorizontalAlignment="Left" Margin="10,45,0,0" VerticalAlignment="Top" Width="259" IsEnabled="False"/>
                            <Button x:Name="button1_Copy2" Content="Export Workspace" HorizontalAlignment="Left" Margin="10,79,0,0" VerticalAlignment="Top" Width="259" IsEnabled="False"/>
                            <Button x:Name="button1_Copy3" Content="Meteor Pack Workspace" HorizontalAlignment="Left" Margin="10,115,0,0" VerticalAlignment="Top" Width="259" IsEnabled="False"/>
                            <Button x:Name="button1_Copy4" Content="Backup Workspace" HorizontalAlignment="Left" Margin="10,149,0,0" VerticalAlignment="Top" Width="259" IsEnabled="False"/>
                        </Grid>
                    </GroupBox>
                    <Button x:Name="button3" Content="Add Workspace" HorizontalAlignment="Left" Margin="10,230,0,0" Grid.Row="1" VerticalAlignment="Top" Width="140" Click="add_workspace"/>
                </Grid>
            </TabItem>
            <TabItem x:Name="Configuration" Header="TabItem" Background="{x:Null}" BorderBrush="{x:Null}" Visibility="Collapsed">
                <Grid x:Name="config_grid">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <GroupBox x:Name="region_select" Header="Region Select" HorizontalAlignment="Left" Height="68" Margin="0,10,0,0" VerticalAlignment="Top" Width="824" Foreground="White">
